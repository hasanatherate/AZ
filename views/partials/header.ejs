          "areaServed": "IN",
          "availableLanguage": ["en", "hi"]
        },
        {
          "@type": "ContactPoint",
          "telephone": "+971-52-948-9322",
          "contactType": "customer service",
          "areaServed": "AE",
          "availableLanguage": ["en", "ar"]
        }
      ],
      "email": "info@allzonecorp.in",
      "sameAs": []
    }
    </script>
    
    <style>
        /* Base Critical Styles */
        :root {--primary-color: #D4AF37;--secondary-color: #FFD700;--dark-bg: #1a1a1a;--transition: all 0.3s ease;}
        body {font-family: 'Poppins', sans-serif;line-height: 1.6;color: #D4AF37;overflow-x: hidden;background: #000000;min-height: 100vh;width: 100%;position: relative;padding-top: 140px;} /* CRITICAL: Fixed Header Padding */
        .container {max-width: 1200px;margin: 0 auto;padding: 0 20px;width: 100%;}
        /* Fixed Header Styles */
        .sleek-header {background: rgba(0, 0, 0, 0.95);backdrop-filter: blur(10px);box-shadow: 0 2px 20px rgba(212, 175, 55, 0.2);position: fixed;top: 0;left: 0;right: 0;width: 100%;z-index: 1000;border-bottom: 1px solid rgba(212, 175, 55, 0.3);transition: transform 0.3s ease-in-out;} /* CRITICAL: Fixes position */
        .header-content {display: flex;justify-content: space-between;align-items: center;padding: 1rem 0;}
        .logo {display: flex;align-items: center;text-decoration: none;}
        .logo-container {display: flex;align-items: center;gap: 1rem;}
        .logo-image {width: 50px;height: 50px;border-radius: 8px;}
        .logo-text h1 {font-size: 1.9rem;font-weight: 800;color: #D4AF37;margin: 0 0 0.3rem 0;line-height: 1.1;}
        .logo-text span {color: #FFD700;font-weight: 600;}
        .main-nav {display: flex;align-items: center;gap: 1.5rem;padding: 1rem 0;border-top: 1px solid rgba(255,255,255,0.2);flex-wrap: wrap;}
        /* CRITICAL: Hide dropdowns initially to prevent FOUC */
        .nav-dropdown {position: relative;}
        .dropdown-menu {position: absolute;top: 100%;left: 0;display: none;opacity: 0;visibility: hidden;}
    </style>
    
